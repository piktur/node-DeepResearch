{
  "name": "@piktur/node-deepresearch",
  "version": "1.0.0",
  "main": "dist/app.js",
  "scripts": {
    "build": "pnpm tsc --build --incremental",
    "dev": "node --experimental-strip-types src/agent.ts",
    "search": "node --experimental-strip-types src/test-duck.ts",
    "rewrite": "node --experimental-strip-types src/tools/query-rewriter.ts",
    "lint": "pnpm eslint . --ext .ts",
    "lint:fix": "pnpm eslint . --ext .ts --fix",
    "eval": "node --experimental-strip-types src/evals/batch-evals.ts",
    "serve": "node --experimental-strip-types src/server.ts",
    "tsc:fix": "pnpm ts-fix --tsconfig ./tsconfig.json --ignoreGitStatus --write",
    "test": "pnpm jest",
    "test:watch": "pnpm jest --watch"
  },
  "author": "Jina AI",
  "license": "Apache-2.0",
  "description": "",
  "type": "module",
  "imports": {
    "#src/*.js": {
      "development": "./src/*.ts",
      "production": "./src/*.ts",
      "import": "./dist/*.js"
    },
    "#src/*.json": "./src/*.json"
  },
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "production": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./index.js": {
      "development": "./src/index.ts",
      "production": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./app.js": {
      "development": "./src/app.ts",
      "production": "./src/app.ts",
      "types": "./dist/app.d.ts",
      "import": "./dist/app.js"
    }
  },
  "dependencies": {
    "@ai-sdk/google": "^1.0.0",
    "@ai-sdk/openai": "^1.1.9",
    "ai": "^4.1.26",
    "axios": "^1.7.9",
    "commander": "^13.1.0",
    "cors": "^2.8.5",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.24.1",
    "dotenv": "16.4.7",
    "duck-duck-scrape": "^2.2.7",
    "express": "catalog:",
    "undici": "^7.3.0"
  },
  "devDependencies": {
    "@piktur/ts-fix": "0.1.0",
    "@types/commander": "^2.12.0",
    "@types/cors": "^2.8.17",
    "@types/express": "catalog:",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.10",
    "@types/node-fetch": "^2.6.12",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3"
  },
  "optionalDependencies": {
    "@ai-sdk/google-vertex": "^2.1.12"
  },
  "packageManager": "pnpm@9.12.2"
}
